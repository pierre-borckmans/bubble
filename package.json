{
  "name": "Bubble",
  "version": "1.0.0",
  "description": "CC with a bubble",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/bubbles.icns --prune=true --out=build",
    "deploy": "cp -rf build/Bubble-darwin-x64/Bubble.app /Applications"
  },
  "repository": "https://github.com/pierre-borckmans/bubble",
  "author": "Pierre Borckmans",
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "noble": "^1.9.1",
    "noble-mac": "git://github.com/Timeular/noble-mac.git",
    "xpc-connection": "git://github.com/taoyuan/node-xpc-connection.git"
  }
}
